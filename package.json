{
  "name": "Chatty",
  "main": "built/api.js",
  "scripts": {
    "prestart": "./node_modules/.bin/tsc",
    "start": "node dist/src/index.js",
    "pretest": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest && ./node_modules/.bin/cucumberjs",
    "build": "./node_modules/.bin/webpack"
  },
  "devDependencies": {
    "colors": "^1.1.2",
    "cucumber": "^2.0.0-rc.2",
    "jest": "^17.0.3",
    "ts-loader": "^1.3.0",
    "typescript": "^2.0.10",
    "webpack": "^1.14.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/test/spec/.*Spec\\.(ts)$"
  },
  "dependencies": {
    "inversify": "^3.0.0-rc.1",
    "reflect-metadata": "^0.1.8"
  }
}
